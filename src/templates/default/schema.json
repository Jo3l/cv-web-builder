  "properties": {
    "firstName": {
      "type": "string",
      "title": "First name",
      "default": ""
    },
    "lastName": {
      "type": "string",
      "title": "Last name",
      "ui:options": {
        "description": "Add all your last name"
      },
      "err:required": "Last Name required"
    },
    "bio": {
      "type": "string",
      "title": "Bio",
      "minLength": 10
    },
    "password": {
      "title": "A string value",
      "type": "string",
      "default": "lorem ipsum"
    },
    "telephone": {
      "type": "string",
      "title": "Telephone",
      "minLength": 10
    },
    "technicalKnowledge": {
      "type": "array",
      "title": "Technical Knowledge",
      "items": {
        "title": "Knowledge",
        "type": "string",
        "default": ""
      }
    },
    "aboutYou": {
      "type": "object",
      "title": "About you",
      "properties": {
        "textarea": {
          "type": "string",
          "title": "this textarea will support markdown",
          "ui:options": {
            "type": "textarea"
          }
        }
      }
    }
  }
}
